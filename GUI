import javax.swing.*;

public class GUI extends JFrame {
	
	public GUI(){
		initUI();
	}
	
	private void initUI(){
		setTitle("Water");
		setSize(1000,1000);
		setLocation(null);
		setDefaultCloseOperation(EXIT_ON_CLOSE);
	}
	
	public static void main(String[] args){
		
		JFrame f = new JFrame();
		f.show();
		
	}
}
